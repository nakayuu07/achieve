<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
   <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
   <div class="test">
     <%= link_to user_path(user) do %>
    <h4><strong><%= profile_img(user) %></strong></h4>
     <% end %>
    <%= user.email %>
    <%= render partial: 'follow_form', locals: { user: user} %>
   </div>
 </div>
  <% end %>
</div>
